<% content_for :title do %>Providers<% end %>

<div id='main-container' class='container'>
  <%= render :partial=>'krikri/shared/flash_message' %>

  <h2>Providers</h2>

  <ul>
  <% @providers.each do |provider| %>
    <li>
      <%= link_to provider.value, { controller: 'providers', action: 'show',
        id: provider.value } %>
    </li>
  <% end %>
  </ul>

</div>
